<script>

$(document).ready(function() {

if(window.location.hash.includes("#@")){            
      var hash = window.location.hash.split("#@")[1];
      console.log(hash);
      window.location.replace("index.html#!content/notebook/captures/logs/"+hash+".md");
}

});


</script>

